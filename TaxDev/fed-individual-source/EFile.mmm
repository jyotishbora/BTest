
Public Sub CreateTPGAppR()
    CustomPrintSub = True
    Call DoTPGAppR
End Sub

Public Sub CreateTPGAppR2()
    CustomPrintSub = True
    Call DoTPGAppR2
End Sub

Public Sub CreateTPGAppO()
    CustomPrintSub = True
    Call DoTPGAppO
End Sub

Public Sub DoTPGAppR()
    TriggerForm = USWRALApp
    Call EFile_TPGAppR
End Sub

Public Sub DoTPGAppR2()
    TriggerForm = USWRALApp
    Call EFile_TPGAppR2
End Sub

Public Sub DoTPGAppO()
    TriggerForm = USWRALApp
    Call EFile_TPGAppO
End Sub
